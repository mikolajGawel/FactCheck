{
    "name": "FactCheck",
    "description": "FactCheck pomaga Ci walczyć z fake newsami! Za pomocą AI klasyfikuje treści jako fakty i opinie.\nAutorzy: Mikołaj Gaweł-Kucab i Wiktor Golicz.",
    "version": "1.0.0",
    "manifest_version": 3,
    "icons": {
        "128": "logo.png"
    },
    "action": {
        "default_icon": "logo.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    }
}