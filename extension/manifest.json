{
    "name": "Content scraper",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_icon": "logo.png"
    },
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "<all_urls>",
         "http://localhost:3000/*"
    ],
    "background": {
        "service_worker": "background.js"
    }
}